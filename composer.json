{
    "name": "flashwave/misuzu",
    "description": "Community System",
    "homepage": "https://flashii.net",
    "support": {
        "email": "me@flash.moe",
        "issues": "https://github.com/flashwave/misuzu/issues"
    },
    "require": {
        "php": ">=7.2",
        "flashwave/aitemu": "dev-master#7997543717d996b56938d5050ea497d62eb71f2e",
        "twig/twig": "~2.4",
        "nesbot/carbon": "~1.22",
        "illuminate/database": "~5.5",
        "illuminate/filesystem": "~5.5",
        "illuminate/pagination": "~5.5",
        "doctrine/dbal": "~2.6",
        "swiftmailer/swiftmailer": "~6.0",
        "erusev/parsedown": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Misuzu\\": "src/"
        },
        "files": [
            "utility.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php misuzu_migrate.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
